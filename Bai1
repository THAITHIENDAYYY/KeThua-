class ChuNhat {
  protected double dai, rong;

  public ChuNhat(double dai, double rong) {
    this.dai = dai;
    this.rong = rong;
  }

  public void xuat() {
    System.out.println("Chieu dai: " + dai);
    System.out.println("Chieu rong: " + rong);
  }
}

class Vuong extends ChuNhat {
  private double canh;

  public Vuong(double canh) {
    super(canh, canh);
    this.canh = canh;
  }

  @Override
  public void xuat() {
    System.out.println("Canh: " + canh);
  }
}

class Main {
  public static void main(String[] args) {
    double dai = 5.0;
    double rong = 3.0;
    double canh = 4.0;

    ChuNhat cn = new ChuNhat(dai, rong);
    ChuNhat vu = new Vuong(canh);

    cn.xuat();
    vu.xuat();
  }
}
